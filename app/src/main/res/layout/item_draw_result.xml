            android:textSize="12sp"
            android:fontFamily="@font/inter_semibold"
            app:layout_constraintEnd_toEndOf="@id/result_special_tv"
            app:layout_constraintStart_toEndOf="@id/special_nmbr_1"
            app:layout_constraintTop_toBottomOf="@id/result_special_tv" />

        <TextView
            android:id="@+id/special_nmbr_3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:padding="4dp"
            android:text="1543"
            android:textSize="12sp"
            android:fontFamily="@font/inter_semibold"
            app:layout_constraintEnd_toStartOf="@id/special_nmbr_4"
            app:layout_constraintStart_toStartOf="@id/special_nmbr_1"
            app:layout_constraintTop_toBottomOf="@id/special_nmbr_1" />

        <TextView
            android:id="@+id/special_nmbr_4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:padding="4dp"
            android:text="0745"
            android:fontFamily="@font/inter_semibold"
            app:layout_constraintEnd_toEndOf="@id/special_nmbr_2"
            app:layout_constraintStart_toEndOf="@id/special_nmbr_3"
            app:layout_constraintTop_toBottomOf="@id/special_nmbr_2" />

        <TextView
            android:id="@+id/special_nmbr_5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:padding="4dp"
            android:text="1543"
            android:fontFamily="@font/inter_semibold"
            app:layout_constraintEnd_toStartOf="@id/special_nmbr_6"
            app:layout_constraintStart_toStartOf="@id/special_nmbr_3"
            app:layout_constraintTop_toBottomOf="@id/special_nmbr_3" />

        <TextView
            android:id="@+id/special_nmbr_6"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:padding="4dp"
            android:textSize="12sp"
            android:text="0745"
            android:fontFamily="@font/inter_semibold"
            app:layout_constraintEnd_toEndOf="@id/special_nmbr_4"
            app:layout_constraintStart_toEndOf="@id/special_nmbr_5"
            app:layout_constraintTop_toBottomOf="@id/special_nmbr_4" />

        <TextView
            android:id="@+id/special_nmbr_7"
            android:layout_width="0dp"
