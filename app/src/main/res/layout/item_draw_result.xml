        <TextView
            android:id="@+id/consolation_number_8"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:padding="4dp"
            android:text="0745"
            android:fontFamily="@font/inter_semibold"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="@id/consolation_number_6"
            app:layout_constraintStart_toEndOf="@id/consolation_number_7"
