            android:padding="4dp"
            android:text="0745"
            android:fontFamily="@font/inter_semibold"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="@id/consolation_number_4"
            app:layout_constraintStart_toEndOf="@id/consolation_number_5"
            app:layout_constraintTop_toBottomOf="@id/consolation_number_4" />

        <TextView
            android:id="@+id/consolation_number_7"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:padding="4dp"
            android:textSize="12sp"
            android:text="1543"
            android:fontFamily="@font/inter_semibold"
            app:layout_constraintEnd_toStartOf="@id/consolation_number_8"
            app:layout_constraintStart_toStartOf="@id/consolation_number_5"
