        <TextView
            android:id="@+id/special_nmbr_10"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:padding="4dp"
            android:text="0745"
            android:fontFamily="@font/inter_semibold"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="@id/special_nmbr_8"
            app:layout_constraintStart_toEndOf="@id/special_nmbr_9"
            app:layout_constraintTop_toBottomOf="@id/special_nmbr_8" />

        <TextView
            android:id="@+id/result_consolation_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Consolation"
            android:textSize="12sp"
            android:fontFamily="@font/inter_medium"
            android:textColor="@color/white"
            android:background="@color/purple"
            android:gravity="center_horizontal"
            app:layout_constraintStart_toStartOf="@id/special_nmbr_9"
            app:layout_constraintTop_toBottomOf="@id/special_nmbr_9"
            app:layout_constraintEnd_toEndOf="parent"
            android:padding="4dp"
            />
        <TextView
            android:id="@+id/consolation_number_1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:padding="4dp"
            android:textSize="12sp"
            android:text="1543"
            android:fontFamily="@font/inter_semibold"
            app:layout_constraintEnd_toStartOf="@id/consolation_number_2"
            app:layout_constraintStart_toStartOf="@id/result_consolation_tv"
            app:layout_constraintTop_toBottomOf="@id/result_consolation_tv" />

        <TextView
            android:id="@+id/consolation_number_2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:padding="4dp"
